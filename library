@Library('pipeline_lib') _


pipeline {
    agent any
    stages {
       
            stage('Checkout') {
            steps {
                script {
                    sh("""
                       rm -rf hello-world-war
                       git clone https://github.com/anilgowda47/hello-world-war.git
                       ls -ltr
                    """)
                }
            }
        }

        stage('Build') {
            steps {
                script {
                    build 'install'
                }
            }
        }
    }
}
